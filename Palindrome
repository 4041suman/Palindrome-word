class Main {
    public static void main(String[] args) {
             String a="madam1";
             if(isPalindrome(a)){
                   System.out.println("Palindrome");
             }
             else{
                          System.out.println("Not Palindrome");

             }  
        }
    public static boolean isPalindrome(String a){
                 int l=0;
                 int r=a.length()-1;
                 while(l<r){
                     if(a.charAt(l)!=a.charAt(r)){
                         return false;
                     }
                     l++;
                     r--;
                 }
                 return true;
            }
}
